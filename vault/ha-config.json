{
  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_disable": "true"
    }
  },

  "backend": {
    "dynamodb": {
      "endpoint": "http://dynamodb:8000",
      "access_key": "*",
      "secret_key": "*",
      "ha_enabled": "true",
      "redirect_addr": "http://vault:8200"
    }
  },

  "disable_mlock": true
}
